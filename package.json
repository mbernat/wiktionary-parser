{
  "name": "wiktionary-parser",
  "version": "0.1.0",
  "description": "Wiktionary parser",
  "license": "MIT",
  "esy": {
    "build": "dune build -p #{self.name}",
    "NOTE": "Optional release Section. Customizes result of `esy release`",
    "release": {
      "bin": [
        "stats",
        "load"
      ]
    },
    "buildEnv": {
      "ODOC_SYNTAX": "re"
    }
  },
  "scripts": {
    "stats": "esy x stats",
    "load": "esy x load",
    "doc": "esy dune build @doc"
  },
  "dependencies": {
    "@opam/dune": "*",
    "ocaml": "~4.6.0"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "ocaml": "~4.6.0",
    "@opam/odoc": "*",
    "@opam/markup": "*",
    "@opam/sqlite3": "*",
    "refmt": "*"
  }
}
